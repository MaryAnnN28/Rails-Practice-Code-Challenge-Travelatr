<h1>Create New Blogger</h1>

<%= form_for @blogger do | f | %>
<b>
<%= f.label :name %> <br>
<%= f.text_field :name %> <br><br>

<%= f.label :bio %> <br>
<%= f.text_area :bio, minlength: 30, size: 30 %> <br><br>

<%= f.label :age %> <br>
<%= f.number_field :age  %> <br><br>

<%= f.submit %>



</b>

<% end %>